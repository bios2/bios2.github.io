Cell[BoxData[
 RowBox[{"derivative", "=", 
  RowBox[{"D", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"r", " ", 
        RowBox[{"(", 
         RowBox[{"1", "-", 
