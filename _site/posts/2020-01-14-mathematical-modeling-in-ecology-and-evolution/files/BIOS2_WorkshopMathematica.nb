Cell[TextData[{
 "Differential equations can also be solved numerically using ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 "'s NDSolve routines, which can work even for models that cannot be solved \
analytically."
}], "Text",
 CellChangeTimes->{{3.5144216199985*^9, 3.514421620816608*^9}, {
  3.5144233861823177`*^9, 3.514423486185019*^9}, {3.5144270172409153`*^9, 
  3.514427088047944*^9}},ExpressionUUID->"d7813717-471b-4423-af8b-\
e482c761f031"],

Cell[BoxData[
 RowBox[{
  RowBox[{"logisticD", "[", 
   RowBox[{"r_", ",", "K_", ",", "n0_"}], "]"}], ":=", 
  RowBox[{"NDSolve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{"n", "[", "t", "]"}], ",", "t"}], "]"}], "==", 
       RowBox[{"r", " ", 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          FractionBox[
           RowBox[{"n", "[", "t", "]"}], "K"]}], ")"}], " ", 
        RowBox[{"n", "[", "t", "]"}]}]}], ",", 
      RowBox[{
       RowBox[{"n", "[", "0", "]"}], "\[Equal]", "n0"}]}], "}"}], ",", 
    RowBox[{"n", "[", "t", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"t", ",", "0", ",", "100"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.51442706851635*^9, 3.51442712755998*^9}, {
  3.514427174650832*^9, 
  3.5144272211635733`*^9}},ExpressionUUID->"eab8fe8a-7e93-4137-a706-\
5c11219140cf"],

