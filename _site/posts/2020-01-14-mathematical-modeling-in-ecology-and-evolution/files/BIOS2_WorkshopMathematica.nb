Cell[TextData[{
 "You can use either the discrete-time or the continuous-time model:\n\n\t",
 Cell[BoxData[
  RowBox[{" ", 
   RowBox[{
    RowBox[{"p", "[", 
     RowBox[{"t", "+", "1"}], "]"}], " ", "=", 
    FractionBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "+", "s"}], ")"}], 
      RowBox[{"p", "[", "t", "]"}]}], 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "+", "s"}], ")"}], 
       RowBox[{"p", "[", "t", "]"}]}], "+", 
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{"p", "[", "t", "]"}]}], ")"}]}]]}]}]],
  CellChangeTimes->{{3.513947684629984*^9, 3.5139476954205637`*^9}},
